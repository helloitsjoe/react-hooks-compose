{
  "name": "react-hooks-compose",
  "version": "2.0.8",
  "description": "Compose React Hooks",
  "main": "dist/main.js",
  "scripts": {
    "test": "jest --coverage --testPathPattern=__tests__/index.test.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "watch": "webpack --watch",
    "build": "webpack",
    "prepublishOnly": "rm -rf dist && npm run build && npm t -- --silent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helloitsjoe/react-hooks-compose.git"
  },
  "keywords": [
    "react",
    "hooks",
    "compose",
    "composition",
    "testing"
  ],
  "author": "Joe Boyle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helloitsjoe/react-hooks-compose/issues"
  },
  "files": [
    "/dist",
    "README.md"
  ],
  "husky": {
    "hooks": {
      "pre-push": "npm t -- --silent"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ]
  },
  "homepage": "https://github.com/helloitsjoe/react-hooks-compose#readme",
  "peerDependencies": {
    "react": "^16.8"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@testing-library/react": "^9.4.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-react-simple": "^1.0.2",
    "coveralls": "^3.0.9",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-helloitsjoe": "^1.2.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^3.1.0",
    "jest": "^26.0.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-simple": "^1.5.2"
  }
}
